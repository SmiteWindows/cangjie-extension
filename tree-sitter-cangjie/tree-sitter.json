    "license": "MIT",
    "description": "Cangjie language grammar for tree-sitter",
    "authors": [
      {
        "name": "Cangjie Team",
        "url": "https://gitcode.com/Cangjie/"
      }
    ],
    "links": {
      "repository": "https://gitcode.com/Cangjie/cangjie_test"
    }
  },
  "bindings": {
    "c": true,
    "node": true,
    "rust": true
  }
}